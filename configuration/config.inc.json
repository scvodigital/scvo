{
  "domains": [
    "scvo.local",
    "127.0.0.1",
    "localhost",
    "test.scvo.org"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/test/scvo/",
    "title": "Saltire Awards",
    "accountName": "SCVO account",
    "url": "https://test.saltireawards.scot",
    "twitter": "scvotweet",
    "contacts": {
      "admin": "enquiries@scvo.org.uk",
      "noreply": "noreply@scvo.org.uk",
      "technical": "webhelpdesk@scvo.org.uk"
    },
    "menus": "{:./data/menus.json}",
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      },
      "frontend": {
        "tasks": {
          "{$./frontend/tasks/**/*.*}": ""
        },
        "configs": {
          "{$./frontend/configs/**/*.*}": ""
        },
        "partials": {
          "{$./frontend/partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
        }
      }
    },
    "commonTasks": "{:./data/common-tasks.json}",
    "elasticsearchUrl": "{$./data/elasticsearch-url.txt}"
  }
}