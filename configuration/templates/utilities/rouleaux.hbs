{{#if @root.request.params.query.style ~}}
{{#switch @root.request.params.query.style ~}}
{{#case "stripes" ~}}
{{~>images_rouleaux_stripes color=(concat "#" (default @root.request.params.query.color 'ffffff'))~}}
{{/case ~}}
{{#case "dots" ~}}
{{~>images_rouleaux_dots color=(concat "#" (default @root.request.params.query.color 'ffffff'))~}}
{{/case ~}}
{{#case "solid" ~}}
{{~>images_rouleaux_solid color=(concat "#" (default @root.request.params.query.color 'ffffff'))~}}
{{/case ~}}
{{/switch ~}}
{{else}}
{{~>images_rouleaux_solid color=(concat "#" (default @root.request.params.query.color 'ffffff'))~}}
{{/if ~}}