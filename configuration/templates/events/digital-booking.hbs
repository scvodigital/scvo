<div class="event event-booking">
  <div class="container-fluid">
    {{#with @root.data.primaryResponse.hits.hits.[0]._source ~}}
    <ul class="breadcrumbs">
      <li><a href="/">Home</a></li>
      <li><a href="/digital">Digital</a></li>
      <li><a href="/digital/events">Events</a></li>
      <li><a href="{{url}}">{{{title}}}</a></li>
      <li>Booking</li>
    </ul>
  </div>
  {{> headers_splash_event}}
  <div class="container-fluid page-content">
    <div class="mdc-layout-grid">
      <div class="mdc-layout-grid__inner">
        <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-9-desktop mdc-layout-grid__cell--span-8-tablet mdc-layout-grid__cell--span-4-phone content event-content">
          {{#if booking_url ~}}
          <iframe src="{{booking_url}}?sfid={{Id}}" frameborder="0" width="100%" height="400" style="overflow:hidden"></iframe>
          {{else}}
          {{!-- <iframe src="https://www.surveygizmo.eu/s3/90064254/Event-Booking-Digital-Events?sfid={{Id}}" frameborder="0" width="100%" height="400" style="overflow:hidden"></iframe> --}}
          <script type="text/javascript" >document.write('<script src="http' + ( ("https:" == document.location.protocol) ? "s" : "") + '://www.surveygizmo.eu/s3/90064254/Event-Booking-Digital-Events?sfid={{Id}}&__output=embedjs&__ref=' + escape(document.location.origin + document.location.pathname) + '" type="text/javascript" ></scr' + 'ipt>');</script><style>.sg-survey{display:none; }</style>
          {{/if ~}}
          <p>
            <strong>The information you provide will be stored in our database to allow us to respond to your booking request. <a href="/privacy/training-and-events" target="_blank">Read our full privacy notice</a>.</strong>
          </p>
        </div>
      </div>
    </div>
    {{/with ~}}
  </div>
</div>
