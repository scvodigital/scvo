{
  {{#each @root.data.checkup ~}}
  {{#each questions ~}}
  {{#compare type "select-one" ~}}
  {{{safeStringify (concat 'answers-' (slugify ../name) '-' value)}}}: "{{~#each options ~}}{{~#compare answer (getProperty @root.request.body (concat 'answers-' (slugify ../../name) '-' ../value))~}}{{score}};{{~/compare~}}{{~/each ~}}",
  {{else}}
  {{#compare type "select-many" ~}}
  {{{safeStringify (concat 'answers-' (slugify ../name) '-' value)}}}: "{{~#each options ~}}{{~#each (split (getProperty @root.request.body (concat 'answers-' (slugify ../../name) '-' ../value)) ';') ~}}{{~#compare ../answer this~}}{{../score}};{{~/compare~}}{{~/each ~}}{{~/each ~}}",
  {{/compare ~}}
  {{/compare ~}}
  {{/each ~}}
  {{/each ~}}
  "end": true
}