{
  "name": "coronavirus_page",
  "debug": false,
  "pattern": "/support/coronavirus/:level_1(/:level_2)(/:level_3)(/:level_4)(/:level_5)(/:level_6)(/:level_7)(/:level_8)",
  "tasks": [
    "context.metaData.commonTasks.authNotRequired",
    {
      "name": "primaryResponse",
      "taskModule": "elasticsearch",
      "config": {
        "connectionStringTemplate": "{$../../data/elasticsearch-legacy-url.txt}",
        "elasticsearchConfig": {
          "apiVersion": "5.6"
        },
        "queryTemplates": {
          "index": "{$../../data/elasticsearch-index.txt}",
          "type": "scvo-page",
          "template": ">context.metaData.handlebars.queries.pages_page",
          "noResultsRoute": "four_oh_four"
        }
      },
      "renderer": "handlebars"
    },
    "context.metaData.commonTasks.tfnCoronavirusRelatedPosts",
    {
      "name": "destination",
      "taskModule": "renderLayout",
      "renderer": "handlebars",
      "config": {
        "logic": {
          "if": [
            "default"
          ]
        },
        "output": "body",
        "layouts": {
          "default": {
            "layout": ">context.metaData.handlebars.layouts.default",
            "parts": {
              "head": ">context.metaData.handlebars.templates.head-tags",
              "header": ">context.metaData.handlebars.templates.support_coronavirus_page-header",
              "body": ">context.metaData.handlebars.templates.support_coronavirus_page-body",
              "rightSidebar": ">context.metaData.handlebars.templates.support_coronavirus_page-sidebar",
              "footer": ">context.metaData.handlebars.templates.page-news-footer"
            },
            "contentType": "text/html"
          }
        }
      }
    }
  ]
}