{
  "name": "page_new_members_tv",
  "debug": true,
  "pattern": "/tv/members",
  "tasks": [
    "context.metaData.commonTasks.authNotRequired",
    {
      "name": "primaryResponse",
      "taskModule": "elasticsearch",
      "config": {
        "connectionStringTemplate": "{$../../data/elasticsearch-url.txt}",
        "elasticsearchConfig": {
          "apiVersion": "5.6"
        },
        "queryTemplates": {
          "index": "{$../../data/elasticsearch-index.txt}",
          "type": "scvo-member",
          "template": ">context.metaData.handlebars.queries.members_tv-new"
        }
      },
      "renderer": "handlebars"
    },
    {
      "name": "destination",
      "taskModule": "renderLayout",
      "renderer": "handlebars",
      "config": {
        "logic": {
          "if": [
            "default"
          ]
        },
        "output": "body",
        "layouts": {
          "default": {
            "layout": ">context.metaData.handlebars.layouts.empty",
            "parts": {
              "content": ">context.metaData.handlebars.templates.tv_members"
            },
            "contentType": "text/html"
          }
        }
      }
    }
  ]
}