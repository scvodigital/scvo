{
  "name": "cjs_sign_in",
  "debug": true,
  "pattern": "/jobs/community-jobs-scotland/vacancies/sign-in",
  "tasks": [
    "context.metaData.commonTasks.authNotRequired",
    "context.metaData.commonTasks.cjsAuthNotRequired",
    {
      "name": "destination",
      "taskModule": "renderLayout",
      "renderer": "handlebars",
      "config": {
        "logic": {
          "if": [
            "default"
          ]
        },
        "output": "body",
        "layouts": {
          "default": {
            "layout": ">context.metaData.handlebars.layouts.default",
            "parts": {
              "head": ">context.metaData.handlebars.templates.head-tags",
              "header": ">context.metaData.handlebars.templates.account_auth_cjs-sign-in-header",
              "body": ">context.metaData.handlebars.templates.account_auth_cjs-sign-in-body",
              "footer": ">context.metaData.handlebars.templates.jobs_cjs_footer"
            },
            "contentType": "text/html"
          }
        }
      }
    }
  ]
}
