{
  "name": "redirect_digihelp-digital-checkup",
  "pattern": "/support/digital/digihelp/digital-check-up(/:one)(/:two)(/:three)(:/four)(:/five)(:/six)",
  "tasks": [
    {
      "name": "url",
      "taskModule": "render",
      "renderer": "handlebars",
      "config": {
        "output": "data",
        "template": "https://digital.checkup.scot/"
      }
    },
    {
      "name": "destination",
      "taskModule": "redirect",
      "config": {
        "location": ">data.url",
        "statusCode": 302
      }
    }
  ]
}