{
  "name": "redirect_coronavirus_supporting",
  "pattern": "/coronavirus-covid-19-information-guidance/what-can-we-do-to-protect-support-staff-volunteers-beneficiaries-service-users(/:one)(/:two)(/:three)(:/four)(:/five)(:/six)",
  "tasks": [
    {
      "name": "url",
      "taskModule": "render",
      "renderer": "handlebars",
      "config": {
        "output": "data",
        "template": "/support/covid-19/supporting-your-people{{#if request.params.one}}/{{request.params.one}}{{/if}}{{#if request.params.two}}/{{request.params.two}}{{/if}}{{#if request.params.three}}/{{request.params.three}}{{/if}}{{#if request.params.four}}/{{request.params.four}}{{/if}}{{#if request.params.five}}/{{request.params.five}}{{/if}}{{#if request.params.six}}/{{request.params.six}}{{/if}}"
      }
    },
    {
      "name": "destination",
      "taskModule": "redirect",
      "config": {
        "location": ">data.url",
        "statusCode": 302
      }
    }
  ]
}