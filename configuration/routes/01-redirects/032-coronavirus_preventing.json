{
  "name": "redirect_coronavirus_preventing",
  "pattern": "/coronavirus-covid-19-information-guidance/whats-the-best-way-to-help-prevent-the-spread-of-covid-19(/:one)(/:two)(/:three)(:/four)(:/five)(:/six)",
  "tasks": [
    {
      "name": "url",
      "taskModule": "render",
      "renderer": "handlebars",
      "config": {
        "output": "data",
        "template": "/support/covid-19/preventing-the-spread{{#if request.params.one}}/{{request.params.one}}{{/if}}{{#if request.params.two}}/{{request.params.two}}{{/if}}{{#if request.params.three}}/{{request.params.three}}{{/if}}{{#if request.params.four}}/{{request.params.four}}{{/if}}{{#if request.params.five}}/{{request.params.five}}{{/if}}{{#if request.params.six}}/{{request.params.six}}{{/if}}"
      }
    },
    {
      "name": "destination",
      "taskModule": "redirect",
      "config": {
        "location": ">data.url",
        "statusCode": 302
      }
    }
  ]
}